        */
       function draw() {
        background(redColor, greenColor, blueColor);
        fill(255);
        cricle(x, y, diameter);
        fill(redColor, greenColor, blueColor);
        circle(x, y, 25);
        if (x >=800 x <= 0) {
            movement *= -1;
        }
        x += movement;
       }

