    <script>
        var movement;
        function setup() {
            createCanvas(800, 600);
            movement = Math.floor(Math.random() * 10) + 1;
        }
       function draw() {
        background(redColor, greenColor, blueColor);
        fill(255);
        cricle(x, y, diameter);
        fill(redColor, greenColor, blueColor);
        circle(x, y, 25);
        if (x >=800 x <= 0) {
            movement *= -1;
        }
        x += movement;
       }

       var redColor = 123;
       var greenColor = 39;
       var blueColor = 21;

       var x = 100;
       var y = 200;
       var diameter = 50; 
       function sterup() {
        createCanvas(800, 600);

       } 
       function draw() {
        background(redColor, greenColor, blueColor);
        fill(255);
        circle(x, y, diameter);
        fill(redColor, greenColor, blueColor);
       
        circle(x, y, 25);
        x++;
       }

