        <script> 
        // this function checks the textboxes to see if there is anything in them.  if nothing appears
        // turns the textbox red, gives it focus and points out a required message.
        function checkboxes()
        {
            var firstName = document.getElementById("fName").value;
            window.alert(firstName)
            if (firstName == "")
            {
                document.getElementById("fName").style.backgroundColor = "red";
                document.getElementByIt("fName").focus()
            }
            //need to check for the other textbox ans also remove the red and the required message until
            //something is filled in.
        }
            </script>
           
